    <validationRules>
        <fullName>Must_have_activity_and_hours_before_subm</fullName>
        <active>true</active>
        <description>An activity and the number of hours donated must be entered before you can select this status to close the customer request</description>
        <errorConditionFormula>AND( ISPICKVAL( Status__c , &quot;Resolved - Ready for Approval&quot;), 
OR(ISBLANK(TEXT(Activity__c)),
(ISNULL(Total_Number_of_Hours_Donated_to_Resolve__c))
))</errorConditionFormula>
        <errorMessage>An activity and the number of hours donated must be entered before you can select this status to close the customer request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Select_Referral_Contact_Name</fullName>
        <active>true</active>
        <errorConditionFormula>(ISBLANK(Referral_Contact_Name__c) = True) &amp;&amp;  (Advise_Referrer_of_Outcome__c = True)</errorConditionFormula>
        <errorDisplayField>Referral_Contact_Name__c</errorDisplayField>
        <errorMessage>Select the Referral Contact Name</errorMessage>
    </validationRules>

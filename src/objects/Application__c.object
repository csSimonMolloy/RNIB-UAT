    <validationRules>
        <fullName>Needs_a_criminal_record_check</fullName>
        <active>false</active>
        <errorConditionFormula>AND(NOT(Criminal_Check_OK__c), TEXT( Status__c) = &quot;Successful&quot;)</errorConditionFormula>
        <errorMessage>An Application Without Either An Accepted Criminal Record Check Or An Approved Criminal Record Risk Assessment Cannot Be Set To Successful</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Needs_a_reference_checks_check</fullName>
        <active>false</active>
        <errorConditionFormula>AND(NOT( References_OK__c ), TEXT( Status__c) = &quot;Successful&quot;)</errorConditionFormula>
        <errorMessage>An Application Without Either The Required Number Of Reference Checks or An Approved Reference Risk Assessment Cannot Be Set To Successful</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Needs_a_vehicle_checks_check</fullName>
        <active>false</active>
        <errorConditionFormula>AND(NOT(Vehicle_Check_OK__c), TEXT( Status__c) = &quot;Successful&quot;)</errorConditionFormula>
        <errorMessage>An Application Without An Accepted Check for this Opportunity Cannot Be Set To Successful</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Volunteer_Role_Supply_crim_off_detail</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  ISPICKVAL( Convicted_of_Criminal_Offence__c , &quot;Yes&quot; ), ISBLANK ( Criminal_Offence_Details__c ), ISPICKVAL(Status__c, &quot;Submitted&quot;))</errorConditionFormula>
        <errorDisplayField>Criminal_Offence_Details__c</errorDisplayField>
        <errorMessage>Please supply details of the offence</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Activate_Role</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Activate Role</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)}

if (&apos;{!Application__c.Status__c}&apos; != &quot;Recruitment Checks Complete&quot;){
window.alert(&apos;You can only Activate a Role with the Status of \&apos;Recruitment Checks Complete\&apos;&apos;);
}else {

if ({!Application__c.Can_Activate_Vehicle__c} &amp;&amp; {!Application__c.Can_Activate_References__c} &amp;&amp; {!Application__c.Can_Activate_Criminal_Check__c}) {
if (window.confirm(&apos;If you continue this Application will be set to a Role and the Applicant will be set to an Active Volunteer&apos;)){
sforce.apex.execute(&quot;ApplicationMethods&quot;,&quot;activateApplication&quot;, {applicationId:&quot;{!Application__c.Id}&quot;});
window.location.reload();
}
}else {
window.alert(&apos;Criminal Record Checks, References, and Vehicle Checks must all be satisfied before you can Activate this Role&apos;);
}
}</url>
    </webLinks>

    <validationRules>
        <fullName>Action_After_Description_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Action_taken_after_to_reduce_risk__c, &quot;Yes&quot;), Reduce_Risk_Description__c =&quot;&quot;)</errorConditionFormula>
        <errorMessage>Reduce Risk Description must be filled in</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Property_Damage_Description_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Property_damaged__c,&quot;Yes&quot;), Property_damaged_Description__c =&quot;&quot;)</errorConditionFormula>
        <errorMessage>Property Damage Description must be filled in</errorMessage>
    </validationRules>

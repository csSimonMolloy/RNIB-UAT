    <validationRules>
        <fullName>Criminal_Record_Check_Region_Check</fullName>
        <active>false</active>
        <errorConditionFormula>OR(AND(INCLUDES(Location__c, &quot;Northern Ireland&quot;),   NOT(BEGINS(TEXT(DBS_Check_required__c), &quot;Access&quot;))),
AND(INCLUDES(Location__c, &quot;Scotland&quot;),   NOT(BEGINS(TEXT(DBS_Check_required__c), &quot;PVG&quot;)))
)</errorConditionFormula>
        <errorDisplayField>DBS_Check_required__c</errorDisplayField>
        <errorMessage>Criminal Record Checks not valid for Region selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Either_whatwillvols_or_further_info</fullName>
        <active>false</active>
        <description>Either What will Volunteers be doing multi-select picklist or the What will I be doing text must be filled in</description>
        <errorConditionFormula>AND( (ISBLANK(What_will_I_be_doing__c))  , (ISBLANK(What_will_I_be_doing2__c)))</errorConditionFormula>
        <errorMessage>You must enter information in either the What Will The Volunteer(s) Be Doing? field or What Will They Be Doing - Further Info</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_CRC_required_2_references_required</fullName>
        <active>false</active>
        <errorConditionFormula>AND(NOT(ISPICKVAL(Number_of_References_Required__c, &quot;2&quot;)), NOT(ISPICKVAL( DBS_Check_required__c , &quot;No Check Required&quot;)))</errorConditionFormula>
        <errorDisplayField>Number_of_References_Required__c</errorDisplayField>
        <errorMessage>At least two references are required if the Opportunity requires a Criminal Record Check</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_why_Opportunity_not_Advertised</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISPICKVAL( Do_you_want_your_opportunity_advertised__c ,&quot;No&quot;),  ISBLANK( If_not_why_not__c ) )</errorConditionFormula>
        <errorDisplayField>If_not_why_not__c</errorDisplayField>
        <errorMessage>Please enter a reason why you do not wish the Opportunity to be advertised</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Risk_Assessment</fullName>
        <active>false</active>
        <errorConditionFormula>Risk_Assessment__c  = FALSE</errorConditionFormula>
        <errorDisplayField>Risk_Assessment__c</errorDisplayField>
        <errorMessage>Please check that you have carried out a Risk Assessment</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Role_Templates_GVT</fullName>
        <active>false</active>
        <description>Only GVT can edit template</description>
        <errorConditionFormula>IF(RecordType.Name=&quot;Template&quot;,
IF($User.Role_Template__c=FALSE,
TRUE,
FALSE),
FALSE)</errorConditionFormula>
        <errorMessage>You are not a system administrator so you are unable to edit this template</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Withdrawn_reason</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISPICKVAL(Status__c, &quot;Withdrawn&quot;),  ISBLANK( Withdrawn_Reason__c ))</errorConditionFormula>
        <errorDisplayField>Withdrawn_Reason__c</errorDisplayField>
        <errorMessage>You must add a withdrawn reason</errorMessage>
    </validationRules>
